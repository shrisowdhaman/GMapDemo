<h2> User Details </h2>

<table>
  <tr>
    <td> Name : </td> <td><%= @user.login %> </td>
  </tr>
  <tr>
    <td> Email :</td> <td> <%= @user.email %> </td>
  </tr>
  <tr>
    <td> Address :</td> <td> <%= show_address(@user.user_attribute) %>  </td>
  </tr>
  <tr>
    <td> State :</td> <td> <%= @user.user_attribute.state %></td>
  </tr>
  <tr>
    <td> Country </td> <td> <%= @user.user_attribute.country %> </td>
  </tr>

</table>

<%= gmaps4rails(@json) %>
